#include <stdio.h>

int main(void) {
	// your code goes here
	int T;
	scanf("%d", &T);
	int i, a, b, count = 0;
	for(i = 0; i < T; i++){
	    scanf("%d", &a);
	    while(a > 8){
	        count = count + 1;
	        a = a - 8;
	    }
	    if(a <= 8){
	    if(a == 1){
	        b = 4 + (8 * count);
	        printf("%dLB\n", b);
	    }
	    else if(a == 2){
	         b = 5 + (8 * count);
	        printf("%dMB\n", b);
	    }
	    else if(a == 3){
	         b = 6 + (8 * count);
	        printf("%dUB\n", b);
	    }
	    else if(a == 4){
	         b = 1 + (8 * count);
	        printf("%dLB\n", b);
	    }
	    else if(a == 5){
	         b = 2 + (8 * count);
	        printf("%dMB\n", b);
	    }
	    else if(a == 6){
	         b = 3 + (8 * count);
	        printf("%dUB\n", b);
	    }
	    else if(a == 7){
	         b = 8 + (8 * count);
	        printf("%dSU\n", b);
	    }
	    else {
	         b = 7 + (8 * count);
	        printf("%dSL\n", b);
	    }
	    }
	}
	return 0;
}
